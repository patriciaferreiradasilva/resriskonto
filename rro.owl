        <Literal xml:lang="en">A Generically Dependent Continuant that consists of a collective of Facies.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
